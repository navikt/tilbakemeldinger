{
    "name": "tilbakemeldinger-server",
    "version": "4.10.5",
    "private": true,
    "scripts": {
        "build": "tsc",
        "start": "node -r dotenv/config ./dist/server/server/src/server.js dotenv_config_path=../.env",
        "dev": "npm run build && concurrently \"tsc -w\" \"nodemon -r dotenv/config -w ./dist/server ./dist/server/server/src/server.js dotenv_config_path=../.env\""
    },
    "dependencies": {
        "compression": "1.8.1",
        "csp-header": "6.3.0",
        "dotenv": "17.2.4",
        "express": "5.2.1",
        "express-rate-limit": "8.2.1",
        "jwt-decode": "4.0.0",
        "lru-cache": "11.2.5",
        "node-cache": "5.1.2",
        "openid-client": "5.6.5"
    },
    "devDependencies": {
        "@types/compression": "1.8.1",
        "@types/express": "5.0.6",
        "@types/node": "24.1.0",
        "concurrently": "9.2.1",
        "nodemon": "3.1.11"
    },
    "nodemonConfig": {
        "delay": 500
    }
}
