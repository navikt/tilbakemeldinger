{
    "name": "tilbakemeldinger-server",
    "version": "4.10.4",
    "private": true,
    "scripts": {
        "build": "tsc",
        "start": "node -r dotenv/config ./dist/server/server/src/server.js dotenv_config_path=../.env",
        "dev": "npm run build && concurrently \"tsc -w\" \"nodemon -r dotenv/config -w ./dist/server ./dist/server/server/src/server.js dotenv_config_path=../.env\""
    },
    "dependencies": {
        "compression": "1.7.4",
        "csp-header": "5.2.1",
        "dotenv": "16.4.5",
        "express": "4.19.2",
        "express-rate-limit": "7.2.0",
        "jwt-decode": "4.0.0",
        "lru-cache": "10.2.0",
        "node-cache": "5.1.2",
        "openid-client": "5.6.5"
    },
    "devDependencies": {
        "@types/compression": "1.7.5",
        "@types/express": "4.17.21",
        "@types/node": "20.12.7",
        "concurrently": "8.2.2",
        "nodemon": "3.1.0",
        "vite": "5.2.8"
    },
    "nodemonConfig": {
        "delay": 500
    }
}
