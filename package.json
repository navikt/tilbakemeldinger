{
    "name": "tilbakemeldinger",
    "version": "4.10.5",
    "private": true,
    "author": "Nav",
    "license": "MIT",
    "homepage": "/person/kontakt-oss/tilbakemeldinger",
    "type": "module",
    "packageManager": "pnpm@10.30.1+sha512.3590e550d5384caa39bd5c7c739f72270234b2f6059e13018f975c313b1eb9fefcc09714048765d4d9efe961382c312e624572c0420762bdc5d5940cdf9be73a",
    "scripts": {
        "build": "pnpm run build:client && pnpm run build:ssr && pnpm run build:server",
        "build:client": "vite build --outDir ./server/dist/client",
        "build:ssr": "vite build --outDir ./server/src/_ssr-dist --ssr src/main-server.tsx",
        "build:server": "pnpm --filter tilbakemeldinger-server run build",
        "analyze-bundle": "ANALYZE=true pnpm run build:client",
        "start": "pnpm --filter tilbakemeldinger-server run start",
        "start-local": "cp .env.local-prod .env && pnpm --filter tilbakemeldinger-server run start",
        "lint": "pnpm run lint:eslint && pnpm run lint:types",
        "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"common/**/*.{js,jsx,ts,tsx}\" \"server/src/**/*.{js,jsx,ts,tsx}\"",
        "lint:types": "tsc --noEmit && pnpm --filter tilbakemeldinger-server exec tsc --noEmit",
        "decorator-local": "docker compose up",
        "dev-prepare": "node preact-compat-symlinks.js && cp .env.development .env",
        "dev": "pnpm run dev-prepare && pnpm --filter tilbakemeldinger-server run dev",
        "prepare": "husky || true",
        "test": "node --experimental-vm-modules $(pnpm root)/jest/bin/jest.js --config jest.config.js",
        "test:watch": "node --experimental-vm-modules $(pnpm root)/jest/bin/jest.js --config jest.config.js --watch",
        "test:coverage": "node --experimental-vm-modules $(pnpm root)/jest/bin/jest.js --config jest.config.js --coverage"
    },
    "dependencies": {
        "@floating-ui/react": "0.27.16",
        "@navikt/aksel-icons": "7.32.3",
        "@navikt/ds-css": "7.32.3",
        "@navikt/ds-react": "7.35.1",
        "@navikt/fnrvalidator": "1.3.3",
        "@navikt/nav-dekoratoren-moduler": "3.6.2",
        "@preact/compat": "18.3.1",
        "@prefresh/vite": "2.4.11",
        "date-fns": "4.1.0",
        "downshift": "9.0.12",
        "preact": "10.28.3",
        "preact-render-to-string": "6.6.3",
        "react": "npm:@preact/compat@18.3.1",
        "react-dom": "npm:@preact/compat@18.3.1",
        "react-helmet-async": "2.0.5",
        "react-hook-form": "7.65.0",
        "react-intl": "6.6.8",
        "react-router": "7.13.0",
        "react-router-dom": "7.13.0",
        "vite": "7.2.6",
        "zod": "4.1.12"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.3.3",
        "@eslint/js": "9.39.2",
        "@preact/preset-vite": "2.10.2",
        "@testing-library/jest-dom": "6.9.1",
        "@testing-library/preact": "3.2.4",
        "@types/fetch-mock": "7.3.8",
        "@types/jest": "30.0.0",
        "@types/node": "24.1.0",
        "@types/react": "18.3.18",
        "@types/react-dom": "18.3.5",
        "@typescript-eslint/eslint-plugin": "8.46.2",
        "@typescript-eslint/parser": "8.48.0",
        "eslint": "9.38.0",
        "eslint-plugin-react": "7.37.5",
        "fetch-mock": "11.1.5",
        "globals": "17.0.0",
        "husky": "9.1.5",
        "identity-obj-proxy": "3.0.0",
        "jest": "30.2.0",
        "jest-environment-jsdom": "30.2.0",
        "lint-staged": "16.2.7",
        "prettier": "3.2.5",
        "rollup-plugin-visualizer": "5.12.0",
        "sass": "1.94.2",
        "ts-jest": "29.4.6",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "vite-tsconfig-paths": "6.1.1"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
        "*.{js,jsx,ts,tsx}": "eslint --fix"
    }
}
